{
  "name": "es6-data-structures-and-algorithms",
  "version": "0.0.1",
  "description": "Popular Data Structures and Algorithms done in JavaScript ES6",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "babel-node -- index",
    "test": "export NODE_PATH=./ && nyc --reporter=lcov mocha --compilers js:babel-register --recursive ./__tests__ --timeout 120000 --exit && cat ./coverage/lcov.info | codacy-coverage",
    "lint": "eslint index.js src/**/*.js __tests__/**/*.js",
    "validate": "npm run lint && npm run test",
    "precommit": "npm run validate",
    "prepush": "npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akhenda/es6-data-structures-and-algorithms.git"
  },
  "keywords": [
    "data-structures",
    "algorithms",
    "data-structures-and-algorithms",
    "es6",
    "es2015"
  ],
  "author": "Joseph Akhenda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akhenda/es6-data-structures-and-algorithms/issues"
  },
  "homepage": "https://github.com/akhenda/es6-data-structures-and-algorithms#readme",
  "contributors": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "codacy-coverage": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  }
}
